{{!< default}}
{{#if data.announcements}}
	{{#each data.announcements}}
	<div class='{{announcementClass}}'>
		<h6> {{title}} </h6>
		<p> {{description}} </p>
	</div>
	{{/each}}
	<hr />
{{/if}}


{{#if data.ministries}}
	<h1> Services </h1>
	{{#each data.ministries}}
		{{#if homePage}}
			<p> {{title}} : {{time}} </p>
		{{/if}}
	{{/each}}
	<hr />
{{/if}}


{{#if data.events}}
	<h1> Events </h1>
	{{#each data.events}}
		<p> {{title}} : {{{description}}} </p>
	{{/each}}
{{/if}}
